DEFINE SensorCollection  sensor_collection1, sensor_collection2;

DEFINE ObservationStream  observation_stream1, observation_stream2;

sensor_collection1 =
SELECT *
FROM Sensor, Infrastructure
WHERE Sensor.location_id =Infrastructure.region_id;

sensor_collection2 =
SELECT *
FROM sensor_collection1 sen
WHERE sen.`Sensor.id` LIKE "3141%";

observation_stream1 = SENSORS_TO_OBSERVATION_STREAM(sensor_collection1);
observation_stream2 = SENSORS_TO_OBSERVATION_STREAM(sensor_collection2);

SELECT COUNT(*)
FROM observation_stream1 obs1 RANGE '5 seconds' SLIDE '1 seconds',
 observation_stream2 obs2 RANGE '5 seconds' SLIDE '1 seconds'
WHERE obs1.id = obs2.id;